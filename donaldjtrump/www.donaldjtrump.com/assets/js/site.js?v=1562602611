el).removeClass('hidden');$('#'+relPanel).animatescroll({scrollSpeed:400,padding:136});});$('#forms_field_152 input.text').attr({'type':'tel','autocomplete':'tel','required':'required'}).keydown(function(e){if($.inArray(e.keyCode,[46,8,9,27,13,110,190])!==-1||(e.keyCode==65&&(e.ctrlKey===true||e.metaKey===true))||(e.keyCode==67&&(e.ctrlKey===true||e.metaKey===true))||(e.keyCode==88&&(e.ctrlKey===true||e.metaKey===true))||(e.keyCode>=35&&e.keyCode<=39)){return;}
if((e.shiftKey||(e.keyCode<48||e.keyCode>57))&&(e.keyCode<96||e.keyCode>105)){e.preventDefault();}});$('.dform_text_input.field_zip input.text').attr({'maxlength':5,'autocomplete':'postal-code'}).keydown(function(e){if($.inArray(e.keyCode,[46,8,9,27,13,110,190])!==-1||(e.keyCode==65&&(e.ctrlKey===true||e.metaKey===true))||(e.keyCode==67&&(e.ctrlKey===true||e.metaKey===true))||(e.keyCode==88&&(e.ctrlKey===true||e.metaKey===true))||(e.keyCode>=35&&e.keyCode<=39)){return;}
if((e.shiftKey||(e.keyCode<48||e.keyCode>57))&&(e.keyCode<96||e.keyCode>105)){e.preventDefault();}});$('#rallies .tickets').on('change',function(){var val=$('.tickets select').val();if(val==1){$('.signup_second').hide().find('input').removeAttr('required');}else{$('.signup_second').show().find('input').attr('required','required');}}).find('input:checked').trigger('change');